{"common/EditName.html":"<!--仅编辑名称-->\n<ul class=\"commonDialog editLink\" id=\"editName\"> \n    <li class=\"head\"> \n        <div class=\"title\">编辑名称</div> \n        <div class=\"close\">x</div> \n    </li> \n    <li class=\"body\"> \n        <div class=\"text\">\n            <textarea placeholder=\"填写名称\"></textarea>\n        </div> \n    </li> \n    <li class=\"foot\"> \n        <div class=\"buttons\"> \n            <button class=\"confirm\">确定</button> \n            <button class=\"cancel\">取消</button> \n        </div> \n    </li> \n</ul> ","common/ImageShowcase.html":"<!-- 大图片展示框 -->\n<div class=\"image_showcase\">\n    <img class=\"big_img\" :src=\"currImageSrc\" @load=\"onImageLoad($event)\" :title=\"currImageName\" :alt=\"currImageName\"/>\n    <div class=\"fake_btn arrow left_arrow\" @click=\"prevPage()\" v-show=\"totalPage>1\">&lt;</div>\n    <div class=\"fake_btn arrow right_arrow\" @click=\"nextPage()\" v-show=\"totalPage>1\">&gt;</div>\n    <div class=\"txt_page\">{{currPage}}/{{totalPage}}</div>\n    <div class=\"fake_btn btn_close\" @click.stop=\"onClose()\">x</div>\n</div>","common/Project.html":"<!--项目导航-->\n<div id=\"projectNav\" class=\"commonNav\">\n    <div class=\"user\">\n        <div id=\"uname\" class=\"uname\"></div>\n        <div id=\"umsgs\" class=\"umsgs\">0</div>\n    </div>\n    <div class=\"head\">\n        <ul class=\"tools\">\n            <li type=\"ProfilePanel\">日程</li>\n            <li type=\"ProcessPanel\">进度</li>\n            <li type=\"CollatePanel\">晨会</li>\n        </ul>\n    </div>\n    <div class=\"body\">\n        <ul class=\"menu\">\n            <li did=\"0\">版本</li>\n            <li did=\"-1\">全部</li>\n            <li did=\"1\">策划</li>\n            <li did=\"2\">美术</li>\n            <li did=\"4\">前端</li>\n            <li did=\"5\">后端</li>\n            <li did=\"6\">质检</li>\n            <li did=\"14\">监修</li>\n            <li did=\"16\">工具</li>\n        </ul>\n    </div>\n    <div class=\"foot\">\n        <ul class=\"tools\">\n            <li id=\"projectSer\">筛选</li>\n        </ul>\n        <ul class=\"tools\">\n            <li id=\"saveFile\">保存</li>\n            <li style=\"display:none;\">统计</li>\n        </ul>\n        <ul class=\"tools tpl_edit\">\n            <li>编辑模版</li>\n        </ul>\n        <ul class=\"tools test_fox\" style=\"display:none\">\n            <li>Test Fox</li>\n        </ul>\n    </div>\n</div>\n\n<!--进度条-->\n<div id=\"loading\" class=\"commonLoading\">\n    <div class=\"bar\"></div>\n</div>\n\n<!--内容区域-->\n<div id=\"content\" class=\"commonBody\"></div>\n\n<!--Tips-->\n<div class=\"workTips\" id=\"workTips\"></div>\n\n<!--工作菜单-->\n<div class=\"commonMenu\" id=\"menuDay\">\n    <div class=\"row\" type=\"work\" status=\"0\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">工作</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"finish\" status=\"3\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">完成</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"edit\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">编辑</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row extends\" type=\"status\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">状态</div>\n        <div class=\"dev\">\n            <div class=\"arrow-right\"></div>\n            <div class=\"spread\">\n                <div class=\"row\" type=\"delay\" status=\"1\">\n                    <div class=\"ico\"></div>\n                    <div class=\"txt\">延期</div>\n                    <div class=\"dev\"></div>\n                </div>\n                <div class=\"row\" type=\"optimize\" status=\"5\">\n                    <div class=\"ico\"></div>\n                    <div class=\"txt\">优化</div>\n                    <div class=\"dev\"></div>\n                </div>\n                <div class=\"row\" type=\"wait\" status=\"2\">\n                    <div class=\"ico\"></div>\n                    <div class=\"txt\">等待</div>\n                    <div class=\"dev\"></div>\n                </div>\n                <div class=\"row\" type=\"rest\" status=\"4\">\n                    <div class=\"ico\"></div>\n                    <div class=\"txt\">请假</div>\n                    <div class=\"dev\"></div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"row\" type=\"score\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">评价</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"upload\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">附件</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"clear\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">清空</div>\n        <div class=\"dev\"></div>\n    </div>\n</div>\n\n<!--流程菜单-->\n<div class=\"commonMenu\" id=\"menuLink\">\n    <div class=\"row\" type=\"color\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">背景</div>\n        <div class=\"dev\">\n            <div class=\"arrow-right\"></div>\n            <div class=\"pluginColor\">\n                <div class=\"none\"></div>\n                <div class=\"bg_1\"></div>\n                <div class=\"bg_2\"></div>\n                <div class=\"bg_3\"></div>\n                <div class=\"bg_4\"></div>\n                <div class=\"bg_5\"></div>\n                <div class=\"bg_6\"></div>\n                <div class=\"bg_7\"></div>\n                <div class=\"bg_8\"></div>\n            </div>\n        </div>\n    </div>\n    <div class=\"row\" type=\"edit\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">编辑</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"insert\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">插入</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"forward\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">上移</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"backward\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">下移</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"store\">\n        <div class=\"ico\"></div>\n        <div class=\"txt store_txt\">归档</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"delete\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">删除</div>\n        <div class=\"dev\"></div>\n    </div>\n</div>\n\n<!--人员菜单-->\n<div class=\"commonMenu\" id=\"menuUser\"></div>\n\n<!--功能菜单-->\n<div class=\"commonMenu\" id=\"menuMode\">\n    <div class=\"row\" type=\"color\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">背景</div>\n        <div class=\"dev\">\n            <div class=\"arrow-right\"></div>\n            <div class=\"pluginColor\">\n                <div class=\"none\"></div>\n                <div class=\"bg_1\"></div>\n                <div class=\"bg_2\"></div>\n                <div class=\"bg_3\"></div>\n                <div class=\"bg_4\"></div>\n                <div class=\"bg_5\"></div>\n                <div class=\"bg_6\"></div>\n                <div class=\"bg_7\"></div>\n                <div class=\"bg_8\"></div>\n            </div>\n        </div>\n    </div>\n    <div class=\"row\" type=\"edit\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">编辑</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"insert\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">插入</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"forward\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">上移</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"backward\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">下移</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"store\">\n        <div class=\"ico\"></div>\n        <div class=\"txt store_txt\">归档</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"delete\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">删除</div>\n        <div class=\"dev\"></div>\n    </div>\n</div>\n\n<!--编辑流程-->\n<ul class=\"commonDialog editLink\" id=\"editLink\">\n    <li class=\"head\">\n        <div class=\"title\">编辑流程</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <div class=\"text\">\n            <textarea placeholder=\"填写流程名称\"></textarea>\n        </div>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--编辑功能-->\n<ul class=\"commonDialog editMode\" id=\"editMode\">\n    <li class=\"head\">\n        <div class=\"title\">编辑功能</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <div class=\"text\">\n            <textarea placeholder=\"填写功能名称\"></textarea>\n        </div>\n    </li>\n    <li class=\"body\">\n        <div class=\"row\">\n            <div class=\"num\">版本\n                <span id=\"place_versionSelect\" />\n                <!--<input type=\"text\" class=\"ver\" placeholder=\"版号\" />-->\n            </div>\n            <div class=\"div_version_edit\">\n                <button class=\"version_edit\" title=\"编辑版本\">编辑</button>\n            </div>\n        </div>\n    </li>\n    <li class=\"body tpl_li\">\n        <div class=\"row\">\n            <div class=\"tpl_label\">模板\n                <span id=\"place_tplModeSelect\" />\n            </div>\n            <div class=\"div_tpl_edit\">\n                <button class=\"tpl_edit\" title=\"编辑模板\">编辑</button>\n            </div>\n        </div>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--编辑评价-->\n<ul class=\"commonDialog editScore\" id=\"editScore\">\n    <li class=\"head\">\n        <div class=\"title\">编辑评价</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <div class=\"text\">\n            <textarea placeholder=\"填写评价说明\"></textarea>\n        </div>\n    </li>\n    <li class=\"body\">\n        <div class=\"row select\">质量\n            <select>\n                <option value=\"0\">选择</option>\n                <option value=\"30\">差</option>\n                <option value=\"60\">正常</option>\n                <option value=\"70\">好</option>\n            </select>\n        </div>\n        <div class=\"row select\">效率\n            <select>\n                <option value=\"0\">选择</option>\n                <option value=\"30\">低</option>\n                <option value=\"60\">正常</option>\n                <option value=\"70\">高</option>\n            </select>\n        </div>\n        <div class=\"row select\">态度\n            <select>\n                <option value=\"0\">选择</option>\n                <option value=\"30\">差</option>\n                <option value=\"60\">正常</option>\n                <option value=\"70\">好</option>\n            </select>\n        </div>\n    </li>\n    <li class=\"body\">\n        <div class=\"fileBox\">\n        </div>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--编辑日程-->\n<ul class=\"commonDialog editInfo\" id=\"editInfo\">\n    <li class=\"head\">\n        <div class=\"title\">编辑日程</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <div class=\"text\">\n            <textarea placeholder=\"填写日程安排\"></textarea>\n        </div>\n    </li>\n    <li class=\"body\">\n        <div class=\"row\">\n            <div class=\"num\">\n                分子\n                <input type=\"text\" class=\"min\" maxlength=\"3\" /> 分母\n                <input type=\"text\" class=\"max\" maxlength=\"3\" /> 标签\n                <input type=\"text\" class=\"tag\" maxlength=\"1\" />\n            </div>\n        </div>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--晨会筛选-->\n<ul class=\"commonDialog stepFilter\" id=\"workFilter\">\n    <li class=\"head\">\n        <div class=\"title\">内容筛选</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <ul class=\"search\">\n            <li>\n                <div class=\"name\">开始日期</div>\n                <div class=\"info\">\n                    <input type=\"text\" class=\"date\" placeholder=\"选择开始日期\" name=\"BeginDate\" readonly />\n                </div>\n            </li>\n            <li>\n                <div class=\"name\">结束日期</div>\n                <div class=\"info\">\n                    <input type=\"text\" class=\"date\" placeholder=\"选择结束日期\" name=\"EndDate\" readonly />\n                </div>\n            </li>\n        </ul>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--动态dom 插入到这个标签的前或后-->\n<ul class=\"commonDialog\" id=\"dynamicDom\">\n</ul>\n\n<!--时间控件-->\n<div class=\"dateTime\" id=\"dateTime\"></div>\n\n<!--用户查询-->\n<ul class=\"searchUser\" id=\"searchUser\"></ul>\n\n<!--晨会菜单-->\n<div class=\"commonMenu\" id=\"menuStep\">\n    <div class=\"row\" type=\"finish\" inspect=\"1\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">完成</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"last\" inspect=\"2\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">持续</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"defer\" inspect=\"3\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">未完</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"cancel\" inspect=\"0\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">取消</div>\n        <div class=\"dev\"></div>\n    </div>\n</div>\n\n<!--晨会菜单-->\n<div class=\"commonMenu\" id=\"menuExtra\">\n    <div class=\"row\" type=\"edit\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">编辑</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"delete\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">删除</div>\n        <div class=\"dev\"></div>\n    </div>\n</div>\n\n<!--编辑晨会-->\n<ul class=\"commonDialog addStep\" id=\"addStep\">\n    <li class=\"head\">\n        <div class=\"title\">晨会内容</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <div class=\"text\">\n            <textarea placeholder=\"填写内容\"></textarea>\n        </div>\n    </li>\n    <li class=\"body\">\n        <div class=\"row inspect\">\n            <div class=\"check_1\">完成</div>\n            <div class=\"check_2\">持续</div>\n            <div class=\"check_3\">未完</div>\n        </div>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--警告提示-->\n<ul class=\"commonDialog commonWarning\" id=\"warning\">\n    <li class=\"head\">\n        <div class=\"title\">删除警告</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <div class=\"tips\"></div>\n    </li>\n    <li class=\"foot\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </li>\n</ul>\n\n<!--发布菜单-->\n<div class=\"commonMenu\" id=\"pubMenu\">\n    <div class=\"row\" type=\"begin\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本开始</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"end\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本完结</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"seal\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本封存</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"delay\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本延期</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"pub\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本发布</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"summary\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本总结</div>\n        <div class=\"dev\"></div>\n    </div>\n    <div class=\"row\" type=\"del\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">版本删除</div>\n        <div class=\"dev\"></div>\n    </div>\n</div>\n<!--通知展开-->\n<ul class=\"commonDialog extraNotice\" id=\"extraNotice\">\n    <li class=\"head\">\n        <div class=\"title\">通知列表</div>\n        <div class=\"close\">x</div>\n    </li>\n    <li class=\"body\">\n        <ul class=\"list\"></ul>\n    </li>\n</ul>\n<div id=\"commonMini\" class=\"commonMini\"></div>","common/PullDownMenu.html":"<div class=\"commonMenu\">\n\t<div class=\"row\" type=\"all\" v-for=\"item in itemList\" @click=\"onClick(item)\">\n\t\t<div class=\"ico\"></div>\n\t\t<div class=\"txt\" style=\"white-space:nowrap; overflow:hidden; text-overflow:ellipsis;\">{{item.Label}}</div>\n\t\t<div class=\"dev\"></div>\n\t</div>\n</div>","process/FilterItemCheckBox.html":"<li class=\"FilterItemCheckBox\">\n    <div class=\"name\">{{item.Name}}</div>\n    <!--<div class=\"info\"><input type=\"text\" placeholder=\"输入版本号\" name=\"Vid\" /></div>-->\n    <div class=\"info\" :style=\"{'width': (item.ShowLen==-1?'400px':'350px')}\"><!-- 400-350是留给`更多`按钮的宽度 -->\n        <!-- <button class=\"btnSelectAll\">全选</button> -->\n        <span class=\"span_checkbox cursor_pointer user_select_none\" style=\"display: inline-block;margin-right: 6px\" :style=\"{'min-width': (item.ShowLen==-1?'45px':'64px')}\" v-for=\"input,index in item.Inputs\" :key=\"input.Value\" v-if=\"item.ShowLen==-1 || index<item.ShowLen\" @click=\"input.Checked=!input.Checked\" @mouseover=\"$emit('checkBoxMouseEnter',$event,item,input)\" @mouseout=\"$emit('checkBoxMouseOut')\">\n            <input type=\"checkbox\" :name=\"item.InputName\" :value=\"input.Value\" v-model=\"input.Checked\"/>\n            <span class=\"checkbox_name\">{{input.Label}}</span>\n        </span>\n    </div>\n    <div class=\"display_inline_block cursor_pointer user_select_none\"  style=\"vertical-align: top;\" @click=\"item.ShowLen=(item.ShowLen==item.ShowLenMin?item.ShowLenMax:item.ShowLenMin)\" v-if=\"item.ShowLen>-1\"><button class=\"btnMore\" v-text=\"(item.ShowLen==item.ShowLenMin?'更多▽':'收起△')\"></button></div>\n</li>","process/FilterItemTextField.html":"<li class=\"FilterItemTextField\">\n    <div class=\"name\">{{item.Name}}</div>\n    <div class=\"info\">\n        <input type=\"text\" :name=\"item.InputName\" :placeholder=\"item.Placeholder\" v-model=\"item.Value\"\n            @keyup=\"$emit('inputChange',$event,item)\" @click=\"$emit('inputChange',$event,item)\">\n        <!-- <div class=\"prompt\">{{item.Prompt}}</div> -->\n    </div>\n</li>","process/ProcessFilter.html":"<!--进度筛选-->\n<div class=\"commonDialog stepFilter ProcessFilter\">\n\t<div class=\"head\">\n\t\t<div class=\"title\">内容筛选</div>\n\t\t<div class=\"close\" @click=\"onClose()\">x</div>\n\t</div>\n\t<div class=\"body\">\n\t\t<ul>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">日<!-- 防止换行符占位 必须前后紧贴 不要换行\n\t\t\t\t\t --><span style=\"visibility: hidden;\">占位</span>期</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"date\" title=\"选择开始日期\" placeholder=\"选择开始日期\" name=\"BeginDate\" readonly v-model=\"beginDate\" @click=\"onClickDate($event,1)\" /> ~\n\t\t\t\t\t<input type=\"text\" class=\"date\" title=\"选择结束日期\" placeholder=\"选择结束日期\" name=\"EndDate\" readonly v-model=\"endDate\" @click=\"onClickDate($event,2)\"/>\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<FilterItemCheckBox v-if=\"vid\" :item=\"vid\" @checkBoxMouseEnter=\"onCheckBoxMouseEnter\" @checkBoxMouseOut=\"onCheckBoxMouseOut\"></FilterItemCheckBox>\n\t\t\t<FilterItemTextField v-if=\"modeName\" :item=\"modeName\"></FilterItemTextField>\n\t\t\t<FilterItemCheckBox v-if=\"modeStatus\" :item=\"modeStatus\"></FilterItemCheckBox>\n\t\t\t<FilterItemTextField v-if=\"linkName\" :item=\"linkName\"></FilterItemTextField>\n\t\t\t<FilterItemTextField v-if=\"linkUserName\" :item=\"linkUserName\" @inputChange=\"onInputChange\"></FilterItemTextField>\n\t\t\t<FilterItemCheckBox v-if=\"linkStatus\" :item=\"linkStatus\"></FilterItemCheckBox>\n\t\t\t<FilterItemCheckBox v-if=\"linkUserDid\" :item=\"linkUserDid\"></FilterItemCheckBox>\n\t\t\t<FilterItemCheckBox v-if=\"workStatus\" :item=\"workStatus\"></FilterItemCheckBox>\n\t\t\t<FilterItemCheckBox v-if=\"workFile\" :item=\"workFile\"></FilterItemCheckBox>\n\t\t</ul>\n\t\t<!-- backup -->\n\t\t<ul class=\"search\" style=\"border: 1px solid #FF0000;display:none;\">\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">功能版本</div>\n\t\t\t\t<!--<div class=\"info\"><input type=\"text\" placeholder=\"输入版本号\" name=\"Vid\" /></div>-->\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"select\" stype=\"Vid\" readonly value=\"版本号\">\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">功能名称</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" placeholder=\"输入功能名\" name=\"ModeName\" />\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">功能归档</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"select\" stype=\"ModeStatus\" readonly value=\"进行中的\" />\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">开始日期</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"date\" placeholder=\"选择开始日期\" name=\"BeginDate\" readonly />\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">结束日期</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"date\" placeholder=\"选择结束日期\" name=\"EndDate\" readonly />\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">流程名称</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"\" placeholder=\"输入流程名\" name=\"LinkName\" />\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t\t<li>\n\t\t\t\t<div class=\"name\">流程归档</div>\n\t\t\t\t<div class=\"info\">\n\t\t\t\t\t<input type=\"text\" class=\"select\" stype=\"LinkStatus\" readonly value=\"进行中的\" />\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t</ul>\n\t</div>\n\t<div class=\"foot\">\n\t\t<div class=\"prompt\">\n\t\t\t\t[功能名称][流程名称][流程负责]<br/>可以输入多个值, 用`空格`分割\n\t\t</div>\n\t\t<div class=\"buttons\">\n\t\t\t<button class=\"confirm\" @click=\"onSubmit()\">确定</button>\n\t\t\t<button class=\"cancel\" @click=\"onClose()\">取消</button>\n\t\t\t<button class=\"confirm\" @click=\"onReset()\">重置</button>\n\t\t</div>\n\t</div>\n</div>","template/EditTplModeDetail.html":"<!-- 编辑模板-功能中的流程列表 -->\n<div class=\"commonDialog editTplMode editTplModeDetail\" id=\"editTplModeDetail\">\n    <div class=\"head\">\n        <div class=\"title\">编辑模板-{{mode.Name}}</div>\n        <div class=\"close\">x</div>\n    </div>\n    <transition-group name=\"list-complete\" tag=\"ul\"  :class=\"[mode.Links.length<=10?'scroll_y_hidden':'scroll_y_auto']\">\n        <li class=\"body list-complete-item\" v-for=\"(link,index) in mode.Links\" :key=\"link.Tlid\">\n            <input type=\" text \" class=\"ver \" placeholder=\"空 \" :value=\"link.Name \" :id=\" 'editTplModeDetail_'+link.Tlid+\n            '_name' \" @blur=\"onEditName($event,link.Tlid) \"\n                @keyup.enter=\"onEditName($event,link.Tlid) \" />\n            <button class=\"confirm btnSmall2 \" title=\"修改负责人所属的部门 \" @click=\"onChangeDid($event,link.Tlid) \">{{link.Did|didName}}</button>\n            <button :class=\"{ 'confirm':true, 'btnSmall1':true, 'button-disabled':index==0} \" title=\"排序上移 \" @click=\"onSortUp($event,link.Tlid) \"\n                :disabled=\"index==0 \">↑</button>\n            <button :class=\"{ 'confirm':true, 'btnSmall1':true, 'button-disabled':index>=(mode.Links.length-1)}\" title=\"排序下移\" @click=\"onSortDown($event,link.Tlid)\" :disabled=\"index>=(mode.Links.length-1)\">↓</button>\n            <button :class=\"{'confirm':true,'btnSmall1':true}\" title=\"删除\" @click=\"onDel($event,link.Tlid)\">X</button>\n        </li>\n    </transition-group>\n    <div class=\"body\" v-if=\"mode.Links.length<50\">\n        <input type=\"text\" class=\"ver\" placeholder=\"新流程名称\" v-model=\"newName\" @keyup.enter=\"onAdd\" />\n        <button class=\"confirm btnSmall2\" @click=\"onChangeDid($event,-1)\">{{newDid|didName}}</button>\n        <button class=\"confirm btnSmall3\" @click=\"onAdd\">增加</button>\n    </div>\n</div>","template/EditTplModeList.html":"<!-- 编辑模板-功能列表 -->\n<div class=\"commonDialog editTplMode\" id=\"editTplModeList\">\n    <div class=\"head\">\n        <div class=\"title\">编辑模板-功能列表&nbsp;&nbsp;({{modes.length}}/5)</div>\n        <div class=\"close\">x</div>\n    </div>\n    <transition-group name=\"list-complete\" tag=\"ul\">\n        <li class=\"body list-complete-item\" v-for=\"mode in modes\" :key=\"mode.Tmid\">\n            <input type=\"text\" class=\"ver\" :class=\"{'listOpenBorder':mode.Tmid==showTmid}\" placeholder=\"空\" :value=\"mode.Name\" :id=\"'editTplModeList_'+mode.Tmid+'_name'\" @blur=\"onEditName($event,mode.Tmid)\"\n                @keyup.enter=\"onEditName($event,mode.Tmid)\" />\n            <button class=\"confirm btnSmall3 btnEdit\" :class=\"{'listOpenBorder':mode.Tmid==showTmid,'listOpenBg':mode.Tmid==showTmid}\" title=\"编辑功能内流程列表\" v-on:click=\"onEdit($event,mode.Tmid)\">编辑({{mode.Links.length}})</button>\n            <button class=\"cancel btnSmall1\" title=\"删除\" @click=\"onDel($event,mode.Tmid)\">X</button>\n        </li>\n    </transition-group>\n    <div class=\"body\" v-if=\"modes.length<5\">\n        <input type=\"text\" class=\"ver\" placeholder=\"新功能名称\" v-model=\"newName\" @keyup.enter=\"onAdd\" />\n        <button class=\"confirm btnSmall3\" @click=\"onAdd\">增加</button>\n    </div>\n</div>","template/MenuDepartment.html":"<!-- 选择部门的菜单 -->\n<div class=\"commonMenu\" id=\"menuDepartment\">\n    <div class=\"row extends\" v-for=\"d in departments\" @click=\"onClick(d.Did)\">\n        <div class=\"ico\"></div>\n        <div class=\"txt\">{{d.Name}}</div>\n    </div>\n</div>","template/TplModeSelect.html":"<select id=\"tplModeSelect\">\n    <option value=\"0\">空</option>\n    <option v-for=\"mode in modes\" :value=\"mode.Tmid\" v-if=\"mode.Links.length>0\">{{mode.Name}}({{mode.Links.length}})</option>\n</select>","upload/ProcessWorkFileBox.html":"<ul class=\"fileBox\" style=\"padding: 0px;\" :wid=\"wid\">\n    <!-- 不设置wid, 修改items竟然不刷新 -->\n    <li v-for=\"(item,index) in items\" :key=\"item.tempId\" :class=\"['file',{border_dashed:item.state==0||item.state==1,border_solid:item.state==2||item.state==3}]\" >\n        <!-- :style=\"{visibility:item.state==3?'visible':'hidden'}\"> //改成了 没有数据或空数据 也显示附件框 所以这里先注释掉-->\n        <img v-if=\"item.state==3 || item.state==2\" class=\"img\" :src=\"item.src\" style=\"cursor: pointer;\" :title=\"item.Name\" alt=\" \" @click=\"onImageClick(index)\"/>\n        <!-- <img v-show=\"item.state==2\" class=\"img\" :src=\"item.src\" /> -->\n    </li>\n</ul>","upload/UploadWork.html":"<!--work 上传附件-->\n<div class=\"commonDialog uploadWork\" :wid=\"wid\">\n    <div class=\"head\">\n        <div class=\"title\">上传附件</div>\n        <div class=\"close\" @click=\"onClose\">x</div>\n        <input class=\"hidden inputFile\" type=\"file\" @change=\"onChangeInputFile\" multiple=\"multiple\">\n        <!-- accept=\"image/bmp,image/png,image/jpeg\"  -->\n    </div>\n    <transition-group name=\"upload_list\" tag=\"ul\" class=\"fileBox\">\n        <li v-for=\"(item,index) in items\" :key=\"item.TempId\" :class=\"['file','upload_list-item',{border_dashed:item.state==0||item.state==1||item.state==4,border_solid:item.state==2||item.state==3},{cursor_pointer:item.state==1&&uploadingSum==0}]\"\n            @click=\"onSelectFileStart($event,index)\">\n            <!-- 这里cursor_pointer是为了增加按钮添加的,  因为URL状态时有 <a 增加手型标志,所以不需要加 -->\n            <div v-if=\"item.state==1&&uploadingSum==0\" class=\"btn_add1\" title=\"(`选择/拖拽/粘贴`图片)\">增加</div>\n            <div class=\"no-mbp div-img\">\n                <!-- <a v-if=\"item.state==3 || item.state==2\" target=\"_blank\" href=\"javascript:void(0);\" @click=\"onImageClick(index)\"> -->\n                <img v-if=\"item.state==3 || item.state==2\" class=\"img\" :src=\"item.src\" style=\"cursor: pointer;\" :title=\"item.Name\" alt=\" \"\n                    @click=\"onImageClick(index)\" />\n                <!-- </a> -->\n                <!-- <img v-else-if=\"item.state==2\" class=\"img\" :src=\"item.src\" /> -->\n            </div>\n            <button class=\"no-mbp  miniDelete cursor_pointer\" v-if=\"(item.state==2||item.state==3)&&uploadingSum==0\" @click.stop=\"onDeleteFile(index)\" title=\"删除\">x</button>\n            <!-- <div class=\"no-mbp loading\" v-if=\"item.loading\">\n                ▲\n            </div> -->\n        </li>\n    </transition-group>\n    <div class=\"foot\">\n        <div class=\"buttons\">\n            <div class=\"process\" v-if=\"uploadTaskState==2\">\n                <div class=\"process-bar\" :style=\"{'width':(uploadingPercent)+'%'}\" v-if=\"uploadTaskState==2\">\n                </div>\n                <div class=\"process-percent\">\n                    {{uploadingPercent}}%\n                </div>\n            </div>\n            <div class=\"process_complete\" v-if=\"uploadTaskState==3\">\n                <span>\n                    上传完成\n                </span>\n                <br/>\n                <span v-if=\"false\">\n                    (1秒后自动关闭)\n                </span>\n            </div>\n            <button :class=\"['confirm',{'menu_disabled':!submitEnabled}]\" :disabled=\"!submitEnabled\" @click=\"onSubmit\">确定</button>\n            <button class=\"cancel\" @click=\"onClose\">取消</button>\n        </div>\n    </div>\n</div>","version/EditVersionDetail.html":"<!-- 编辑模板-功能中的流程列表 -->\n<div class=\"commonDialog editTplMode editVersion editVersionDetail\">\n    <div class=\"head\">\n        <div class=\"title\">编辑版本-{{version.Ver}}{{version.Name?'-'+version.Name:''}}</div>\n        <div class=\"close\" @click=\"onClose()\">x</div>\n    </div>\n    <ul>\n        <li v-for=\"item,index in version.PublishList\">\n            <div :class=\"['arrow']\" v-if=\"index>0\">➤</div>\n            <button class=\"no-mbp  miniDelete cursor_pointer\" @click.stop=\"onDelete($event,item)\" title=\"清除\" v-if=\"item.DateLine\">x</button>\n            <div class=\"info\">\n                <input type=\"text\" class=\"date\" :title=\"item.ErrorMsg\" :class=\"{'error':Boolean(item.ErrorMsg),'publishOpen':item.Genre==showGenre}\" :placeholder=\"'选择日期'\" :value=\"item.DateLine\" readonly @click=\"onDateClick($event,item,index)\" />\n            </div>\n            <div :class=\"['name sk_color_'+(index+1)]\" v-text=\"(index+1)+'.'+publishName(index+1)+'日期'\"></div>\n        </li>\n    </ul>\n    <div class=\"foot\" v-if=\"false\">\n        <div class=\"buttons\">\n            <button class=\"confirm\">确定</button>\n            <button class=\"cancel\">取消</button>\n        </div>\n    </div>\n</div>","version/EditVersionList.html":"<!-- 编辑模板-功能列表 -->\n<div class=\"commonDialog editTplMode editVersion editVersionList\">\n    <div class=\"head\">\n        <div class=\"title\">编辑版本-列表&nbsp;&nbsp;</div>\n        <div class=\"close\" @click=\"onClose()\">x</div>\n    </div>\n    <transition-group name=\"list-complete\" tag=\"ul\">\n        <li class=\"body list-complete-item\" v-for=\"item,index in versions\" :key=\"item.Vid\" v-if=\"index<ListShowMax\">\n            <input type=\"text\" class=\"name ver\"  :class=\"{'listOpenBorder':item.Vid==showVid}\" placeholder=\"空\" :value=\"item.Ver\" @blur=\"onEditVer($event,item,index)\" @keyup.enter=\"onEditVer($event,item,index)\">\n            <input type=\"text\" class=\"name\"  :class=\"{'listOpenBorder':item.Vid==showVid}\" placeholder=\"空\" :value=\"item.Name\" @blur=\"onEditName($event,item,index)\" @keyup.enter=\"onEditName($event,item,index)\">\n            <button class=\"confirm btnSmall3 btnEdit\"  :class=\"{'listOpenBorder':item.Vid==showVid,'listOpenBg':item.Vid==showVid}\" title=\"编辑版本内容\" @click=\"onEdit($event,item,index)\">编辑</button>\n            <button :class=\"{ 'confirm':true, 'btnSmall1':true, 'button-disabled':index==0} \" title=\"排序上移 \" @click=\"onSortUp($event,item,index) \"\n                :disabled=\"index==0 \">↑</button>\n            <button :class=\"{ 'confirm':true, 'btnSmall1':true, 'button-disabled':index>=(versions-1)}\" title=\"排序下移\" @click=\"onSortDown($event,item,index)\" :disabled=\"index>=(versions.length-1)\">↓</button>\n            <button class=\"cancel btnSmall1\" title=\"删除\" @click=\"onDel($event,item,index)\">X</button>\n        </li>\n    </transition-group>\n    <div class=\"body\">\n        <input type=\"text\" class=\"name ver\" placeholder=\"新版本号\" v-model=\"newVer\" @keyup.enter=\"onAddVer(true)\" @blur=\"onAddVer(false)\">\n        <input type=\"text\" id=\"editVersionList_newName\" class=\"name\" placeholder=\"新版本名称\" v-model=\"newName\" @keyup.enter=\"onAdd()\">\n        <button class=\"confirm btnSmall3\" @click=\"onAdd()\">增加</button>\n    </div>\n</div>","version/TableHeaderTooltip.html":"<div class=\"table_header_tooltip\">\n    <div style=\"margin-bottom: 4px;margin-left: 4px;font-size: 16px;\" v-if=\"currDateLine\">\n        日期:\n        <span style=\"font-weight: bold;\">{{currDateLine}}</span>\n    </div>\n    <div style=\"margin-bottom: 10px;margin-left: 6px;\" v-if=\"items.length!=1\">\n        <span style=\"color:#FF0000;\" v-if=\"items.length>1\">警告:有{{items.length}}个版本日期</span>\n        <span v-if=\"items.length==0\">未设置版本</span>\n    </div>\n    <div v-if=\"items.length>=1\">\n        <div v-for=\"item,index in items\" style=\"display: inline-block;width: auto;padding-left: 6px;\" :class=\"{'border_right_vline':index<items.length-1}\">\n            <span style=\"display:inline-block;width:inherit; font-size: 16px;margin-bottom: 6px;\" class=\"text-overflow-hide versionFullname\">版本: {{GetVersionFullname(item.version)}}</span>\n            <br/>\n            <ul class=\"no-mbp\">\n                <li v-for=\"pItem,pIndex in item.version.PublishList\" v-if=\"Boolean(pItem.DateLine)\" class=\"list-style-type-none\" :class=\"{'font_bold':item.publish && pItem.Genre==item.publish.Genre}\">\n                    <span :class=\"['sk_color_'+pItem.Genre]\" style=\"width: 38px;\">{{GetPublishName(pItem.Genre)}}: </span>\n                    <span :class=\"{'font_bold':item.publish && pItem.Genre==item.publish.Genre,'font_color_black3':item.publish && pItem.Genre!=item.publish.Genre}\" style=\"width: 82px;\">{{pItem.DateLine||'空'}}</span>\n                    <span v-if=\"items.length==1 && pItem.SubDayCount!=0\" style=\"font-weight: normal;\">({{pItem.SubDayCount\n                        <0? -pItem.SubDayCount+'天前': pItem.SubDayCount+'天后'}})</span>\n                </li>\n            </ul>\n        </div>\n    </div>\n    <div style=\"color:rgb(151, 151, 151);padding-top: 10px; text-align: right;\" v-if=\"IsWrite\">\n        点击左键编辑\n    </div>\n</div>","version/VersionSelect.html":"<select id=\"versionSelect\">\n    <option value=\"0\">空</option>\n    <option v-for=\"version,index in versions\" :value=\"version.Vid\" :key=\"version.Vid\" v-if=\" version.Vid==CurrVid || (index<ListShowMax && version.Ver)\">{{version.Ver+(version.Name?'-'+version.Name:'')}}</option>\n</select>"}